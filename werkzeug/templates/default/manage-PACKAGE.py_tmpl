#!/usr/bin/env python
# -*- coding: $FILE_ENCODING -*-
import $PACKAGE
from werkzeug import script

# development configuration
config = {}

def make_app():
    return script.make_app(config)

# script actions
action_runserver = script.make_runserver(make_app, use_reloader=True)

if __name__ == '__main__':
    script.run()
